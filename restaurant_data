                                    class="btn btn-green submit-login--continue js-auth__sign-in-button"
                                    data-event="account.signin.email{{#isCheckoutRegis}}.book{{/isCheckoutRegis}}{{#signInLowerPrice}}.promorate{{/signInLowerPrice}}"
