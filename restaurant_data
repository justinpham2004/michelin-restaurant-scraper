    <script type="text/javascript">

    /**
     * Validate auth expiry
     * @param authObject the auth object from local storage
     * @returns {boolean}
     */
    var validateAuthExpiry = function (authObject) {
        if (!authObject) return false;
        try {
            if (authObject.hasOwnProperty("expires_in")) {
                var date = new Date();
                if (date.getTime() < authObject.expires_in) {
                    return true;
                }
            }
        } catch (e) {
            // noop
        }
        return false;
    };

    /**
     * Parses auth token/jwt
     * @param token the token/jwt
     * @returns {any}
     */
    var parseAuthToken = function (token) {
        var base64Url = token.split('.')[1];
        var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
        var jsonPayload = decodeURIComponent(atob(base64).split('').map(function (c) {
            return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
        }).join(''));

        return JSON.parse(jsonPayload);
    };

    /**
     * Gets auth data
     * @returns {{}|{user_id: *, jwt: *}}
     */
    var getAuthData = function () {
        try {
            var authStorage = localStorage.getItem("auth");
            var authObject = JSON.parse(authStorage);
            if (validateAuthExpiry(authObject)) {
                var authPayload = parseAuthToken(authObject.jwt);
                return {
                    jwt: authObject.jwt,
                    user_id: authPayload.user_id
                };
            }
        } catch (e) {
            // noop
        }
        return {};
    };

    /**
     * Gets user id
     * @returns {string|null}
     */
    var getUserId = function () {
        var _data = getAuthData();
        return _data && _data.user_id ? String(_data.user_id) : null;
    };

    window.USER_ID = getUserId();
    window.IS_SIGNED_IN = window.USER_ID && window.USER_ID.length > 0;
    if (window.IS_SIGNED_IN) {
        window.IS_PLUS_MEMBER = localStorage.getItem("is_plus_member") === '1';
    }

</script>


    
