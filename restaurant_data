    dLayer['isSignedIn'] = window.IS_SIGNED_IN;
    dLayer['isTabletPlusMember'] = !!window.IS_PLUS_MEMBER;
    if (window.IS_SIGNED_IN) dLayer['customerid'] = window.USER_ID;

